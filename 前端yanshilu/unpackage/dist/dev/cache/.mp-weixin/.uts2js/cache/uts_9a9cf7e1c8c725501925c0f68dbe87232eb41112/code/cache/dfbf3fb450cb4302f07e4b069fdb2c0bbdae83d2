{"code":"import { defineComponent } from \"vue\";\n/**\n * @description 底部导航栏组件\n */\nimport { pageMap } from '@/config.js';\nexport default defineComponent({\n    props: {\n        pageName: {\n            type: String,\n            default: 'index'\n        }\n    },\n    methods: {\n        /**\n         * @description 切换到指定页面\n         * @param {string} page - 页面名称\n         */\n        switchPage(page = null) {\n            // 如果已经在当前页面，不执行操作\n            if (page === this.pageName) {\n                return null;\n            }\n            // 跳转前检查目标页面是否存在\n            if (!pageMap[page]) {\n                uni.__f__('error', 'at components/tab-bar/tab-bar.uvue:50', '页面不存在:', page);\n                return null;\n            }\n            try {\n                // 执行页面跳转\n                uni.switchTab({\n                    url: pageMap[page]\n                });\n            }\n            catch (e) {\n                uni.__f__('error', 'at components/tab-bar/tab-bar.uvue:60', 'Tab切换失败:', e);\n            }\n        }\n    }\n});\n//# sourceMappingURL=E:/yanshilu%E5%B0%8F%E7%A8%8B%E5%BA%8F%E5%89%8D%E7%AB%AF/%E5%89%8D%E7%AB%AFyanshilu/components/tab-bar/tab-bar.uvue?vue&type=script&lang.uts.js.map","references":["E:/yanshilu小程序前端/前端yanshilu/config.js"],"uniExtApis":["uni.__f__","uni.switchTab"],"map":"{\"version\":3,\"file\":\"tab-bar.uvue?vue&type=script&lang.uts.js\",\"sourceRoot\":\"\",\"sources\":[\"tab-bar.uvue?vue&type=script&lang.uts\"],\"names\":[],\"mappings\":\";AACC;;GAEG;AACH,OAAO,EAAE,OAAO,EAAE,MAAM,aAAa,CAAC;AAEtC,+BAAe;IACd,KAAK,EAAE;QACN,QAAQ,EAAE;YACT,IAAI,EAAE,MAAM;YACZ,OAAO,EAAE,OAAO;SAChB;KACD;IACD,OAAO,EAAE;QACR;;;WAGG;QACH,UAAU,CAAC,IAAI,OAAA;YACd,kBAAkB;YAClB,IAAI,IAAI,KAAK,IAAI,CAAC,QAAQ,EAAE;gBAC3B,YAAO;aACP;YAED,gBAAgB;YAChB,IAAI,CAAC,OAAO,CAAC,IAAI,CAAC,EAAE;gBACnB,GAAG,CAAC,KAAK,CAAC,OAAO,EAAC,uCAAuC,EAAC,QAAQ,EAAE,IAAI,CAAC,CAAC;gBAC1E,YAAO;aACP;YAED,IAAI;gBACH,SAAS;gBACT,GAAG,CAAC,SAAS,CAAC;oBACb,GAAG,EAAE,OAAO,CAAC,IAAI,CAAC;iB