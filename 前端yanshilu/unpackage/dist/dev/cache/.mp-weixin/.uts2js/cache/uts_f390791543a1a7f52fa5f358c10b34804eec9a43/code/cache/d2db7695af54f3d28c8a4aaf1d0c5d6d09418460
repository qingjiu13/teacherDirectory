se {\n                    // 综合排序，可以根据业务需求实现\n                    // 这里简单实现为按ID排序\n                    result.sort((a, b) => { return a.id - b.id; });\n                }\n            }\n            catch (error) {\n                uni.__f__('error', 'at pages/match/match.uvue:220', '筛选处理异常:', error);\n                // 发生异常时返回原始数据\n                return this.teachers;\n            }\n            return result;\n        }\n    },\n    // 确保此页面能被正确路由\n    onReady() {\n        uni.__f__('log', 'at pages/match/match.uvue:230', 'Match page ready');\n        // 初始化数据\n        this.initFilterData();\n        // 检测当前环境\n        this.detectEnvironment();\n        // 调试下拉菜单状态\n        uni.__f__('log', 'at pages/match/match.uvue:237', '初始下拉菜单状态:', this.activeDropdown);\n    },\n    // 页面显示时添加点击事件监听\n    onShow() {\n        // 添加全局点击事件处理\n        setTimeout(() => {\n            // 只在非App环境中使用document方法\n            if (this.isWebEnv) {\n                // 移除可能存在的旧监听器，避免重复添加\n                document.removeEventListener('click', this.handleGlobalClick);\n                // 添加新的监听器\n                document.addEventListener('click', this.handleGlobalClick);\n                uni.__f__('log', 'at pages/match/match.uvue:249', '全局点击事件监听已添加');\n            }\n        }, 200);\n    },\n    // 页面隐藏时移除点击事件监听\n    onHide() {\n        // 只在非App环境中使用document方法\n        if (this.isWebEnv) {\n            document.removeEventListener('click', this.handleGlobalClick);\n            uni.__f__('log', 'at pages/match/match.uvue:258', '全局点击事件监听已移除');\n        }\n    },\n    // 页面卸载时移除点击事件监听\n    onUnload() {\n        // 只在非App环境中使用document方法\n        if (this.isWebEnv) {\n            document.removeEventListener('click', this.handleGlobalClick);\n        }\n    },\n    watch: {\n        /**\n         * @description 监听筛选条件变化\n         */\n        selectedSchool() {\n            uni.__f__('log', 'at pages/match/match.uvue:273', '学校变更为:', this.selectedSchool);\n            // 这里可以添加额外的处理逻辑，例如加载专业列表等\