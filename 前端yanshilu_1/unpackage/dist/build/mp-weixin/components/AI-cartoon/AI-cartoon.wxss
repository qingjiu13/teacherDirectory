:host{display:flex;flex-direction:column}
.cartoon-icon{position:fixed;z-index:9999;pointer-events:auto;touch-action:none}.icon-image{width:100%;height:100%;border-radius:50%;box-shadow:0 2px 8px rgba(0,0,0,.2);animation:float var(--animation-duration, 2s) ease-in-out infinite;-webkit-user-select:none;user-select:none;-webkit-user-drag:none}@keyframes float{0%{transform:translateY(0)}50%{transform:translateY(calc(var(--float-distance, -10px)))}to{transform:translateY(0)}}
