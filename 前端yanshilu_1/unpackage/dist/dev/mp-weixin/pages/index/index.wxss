@import "../../uvue.wxss";
:host{display:flex;flex-direction:column}

.page-container {
  font-family: Arial, sans-serif;
  padding: 20px;
  text-align: center;
}
.header {
  margin: 50px 0;
}
.main-title {
  font-size: 32px;
  font-weight: bold;
  margin-bottom: 10px;
}
.sub-title {
  font-size: 16px;
  color: #666;
}
.button-area {
  margin-top: 30px;
}
.match-button {
  display: inline-block;
  padding: 10px 20px;
  background: #f0f0f0;
  border-radius: 20px;
  cursor: pointer;
}
.button-text {
  color: #333;
}
