            this.isLoading = true;\n            // 模拟API请求\n            setTimeout(() => {\n                // 模拟数据\n                this.services = [\n                    {\n                        id: 1,\n                        title: '考研数学一对一辅导',\n                        price: 300,\n                        image: '/static/image/tab-bar/default_avatar.png',\n                        description: '针对考研数学难点，提供个性化辅导，帮助你掌握核心解题技巧。'\n                    },\n                    {\n                        id: 2,\n                        title: '计算机专业课指导',\n                        price: 250,\n                        image: '/static/image/tab-bar/default_avatar.png',\n                        description: '数据结构、操作系统、计算机网络、计算机组成原理全面指导。'\n                    },\n                    {\n                        id: 3,\n                        title: '考研复习规划制定',\n                        price: 180,\n                        image: '/static/image/tab-bar/default_avatar.png',\n                        description: '根据个人情况定制复习计划，科学规划时间，提高复习效率。'\n                    }\n                ];\n                this.isLoading = false;\n            }, 500);\n        },\n        /**\n         * @description 发起咨询\n         */\n        startConsultation() {\n            this.isLoading = true;\n            setTimeout(() => {\n                this.isLoading = false;\n                // 使用router.js中的方法跳转到聊天页面\n                Navigator.toChat(this.teacherId);\n            }, 800);\n        }\n    }\n});\n//# sourceMappingURL=E:/yanshilu%E9%87%8D%E6%96%B0%E6%9D%A5%E8%BF%87/%E5%89%8D%E7%AB%AFyanshilu_1/pages/teacher/teacher.uvue?vue&type=script&lang.uts.js.map","references":["E:/yanshilu重新来过/前端yanshilu_1/router/Router.js"],"uniExtApis":[],"map":"{\"version\":3,\"file\":\"teacher.uvue?vue&type=script&lang.uts.js\",\"sourceRoot\":\"\",\"sources\":[\"teacher.uvue?vue&type=script&lang.uts\"],\"names\":[],\"mappings\":\";AACI,OAAO,EAAE,SAAS,EAAE,aAAa,EAAE,MAAM,oBAAoB,CAAA;AAC7D;;;EAGE;AACF,+BAAe;IACX,IAAI