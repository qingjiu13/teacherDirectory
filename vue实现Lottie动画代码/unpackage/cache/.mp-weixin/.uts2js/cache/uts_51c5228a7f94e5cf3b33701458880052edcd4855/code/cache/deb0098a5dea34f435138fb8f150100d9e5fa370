{"code":"import { defineComponent } from \"vue\";\n/**\n * Badge 数字角标\n * @description 数字角标一般和其它控件（列表、9宫格等）配合使用，用于进行数量提示，默认为实心灰色背景\n * @tutorial https://ext.dcloud.net.cn/plugin?id=21\n * @property {String} text 角标内容\n * @property {String} size = [normal|small] 角标内容\n * @property {String} type = [info|primary|success|warning|error] 颜色类型\n * \t@value info 灰色\n * \t@value primary 蓝色\n * \t@value success 绿色\n * \t@value warning 黄色\n * \t@value error 红色\n * @property {String} inverted = [true|false] 是否无需背景颜色\n * @property {Number} maxNum 展示封顶的数字值，超过 99 显示 99+\n * @property {String} absolute = [rightTop|rightBottom|leftBottom|leftTop] 开启绝对定位, 角标将定位到其包裹的标签的四角上\n * \t@value rightTop 右上\n * \t@value rightBottom 右下\n * \t@value leftTop 左上\n * \t@value leftBottom 左下\n * @property {Array[number]} offset\t距定位角中心点的偏移量，只有存在 absolute 属性时有效，例如：[-10, -10] 表示向外偏移 10px，[10, 10] 表示向 absolute 指定的内偏移 10px\n * @property {String} isDot = [true|false] 是否显示为一个小点\n * @event {Function} click 点击 Badge 触发事件\n * @example <uni-badge text=\"1\"></uni-badge>\n */\nexport default defineComponent({\n    name: 'UniBadge',\n    emits: ['click'],\n    props: {\n        type: {\n            type: String,\n            default: 'error'\n        },\n        inverted: {\n            type: Boolean,\n            default: false\n        },\n        isDot: {\n            type: Boolean,\n            default: false\n        },\n        maxNum: {\n            type: Number,\n            default: 99\n        },\n        absolute: {\n            type: String,\n            default: ''\n        },\n        offset: {\n            type: Array,\n            default() {\n                return [0, 0];\n            }\n        },\n        text: {\n            type: [String, Number],\n            default: ''\n        },\n        size: {\n            type: String,\n            default: 'small'\n        },\n        customStyle: {\n            type: Object,\n            default() {\n                return new UTSJSONObject({});\n            }\n        }\n    },\n    data() {\n        return {};\n    },\n    computed: {\n        width() {\n            return String(this.text).length * 8 + 12;\n        },\n        classNames() {\n            const _a = this, inverted = _a.inverted, type = _a.type, size = _a.size, absolute = _a.absolute;\n            return [\n                inverted ? 'uni-badge--' + type + '-inverted' : '',\n                'uni-badge--' + type,\n                'uni-badge--' + size,\n                absolute ? 'uni-badge--absolute' : ''\n            ].join(' ');\n        },\n        positionStyle() {\n            if (!this.absolute)\n                return new UTSJSONObject({});\n            let w = this.width / 2, h = 10;\n            if (this.isDot) {\n                w = 5;\n                h = 5;\n            }\n            const x = `${-w + this.offset[0]}px`;\n            const y = `${-h + this.offset[1]}px`;\n            const whiteList = new UTSJSONObject({\n                rightTop: new UTSJSONObject({\n                    right: x,\n                    top: y\n                }),\n                rightBottom: new UTSJSONObject({\n                    right: x,\n                    bottom: y\n                }),\n                leftBottom: new UTSJSONObject({\n                    left: x,\n                    bottom: y\n                }),\n                leftTop: new UTSJSONObject({\n                    left: x,\n                    top: y\n                })\n            });\n            const match = whiteList[this.absolute];\n            return match ? match : whiteList['rightTop'];\n        },\n        dotStyle() {\n            if (!this.isDot)\n                return new UTSJSONObject({});\n            return new UTSJSONObject({\n                width: '10px',\n                minWidth: '0',\n                height: '10px',\n                padding: '0',\n                borderRadius: '10px'\n            });\n        },\n        displayValue() {\n            const _a = this, isDot = _a.isDot, text = _a.text, maxNum = _a.maxNum;\n            return isDot ? '' : (Number(text) > maxNum ? `${maxNum}+` : text);\n        }\n    },\n    methods: {\n        onClick() {\n            this.$emit('click');\n        }\n    }\n});\n//# sourceMappingURL=E:/%E7%A0%94%E5%B8%88%E5%BD%95%E4%B8%8E%E5%85%B6%E4%BB%96%E4%BA%BA%E5%85%B1%E4%BA%AB%E6%96%87%E4%BB%B6/teacherDirectory/vue%E5%AE%9E%E7%8E%B0Lottie%E5%8A%A8%E7%94%BB%E4%BB%A3%E7%A0%81/uni_modules/uni-badge/components/uni-badge/uni-badge.vue?vue&type=script&lang.uts.js.map","references":[],"uniExtApis":[],"map":"{\"version\":3,\"file\":\"uni-badge.vue?vue&type=script&lang.uts.js\",\"sourceRoot\":\"\",\"sources\":[\"uni-badge.vue?vue&type=script&lang.uts\"],\"names\":[],\"mappings\":\";AACC;;;;;;;;;;;;;;;;;;;;;;;GAuBG;AAEH,+BAAe;IACd,IAAI,EAAE,UAAU;IAChB,KAAK,EAAE,CAAC,OAAO,CAAC;IAChB,KAAK,EAAE;QACN,IAAI,EAAE;YACL,IAAI,EAAE,MAAM;YACZ,OAAO,EAAE,OAAO;SAChB;QACD,QAAQ,EAAE;YACT,IAAI,EAAE,OAAO;YACb,OAAO,EAAE,KAAK;SACd;QACD,KAAK,EAAE;YACN,IAAI,EAAE,OAAO;YACb,OAAO,EAAE,KAAK;SACd;QACD,MAAM,EAAE;YACP,IAAI,EAAE,MAAM;YACZ,OAAO,EAAE,EAAE;SACX;QACD,QAAQ,EAAE;YACT,IAAI,EAAE,MAAM;YACZ,OAAO,EAAE,EAAE;SACX;QACD,MAAM,EAAE;YACP,IAAI,EAAE,KAAK;YACX,OAAO;gBACN,OAAO,CAAC,CAAC,EAAE,CAAC,CAAC,CAAA;YACd,CAAC;SACD;QACD,IAAI,EAAE;YACL,IAAI,EAAE,CAAC,MAAM,EAAE,MAAM,CAAC;YACtB,OAAO,EAAE,EAAE;SACX;QACD,IAAI,EAAE;YACL,IAAI,EAAE,MAAM;YACZ,OAAO,EAAE,OAAO;SAChB;QACD,WAAW,EAAE;YACZ,IAAI,EAAE,MAAM;YACZ,OAAO;gBACN,yBAAO,EAAE,EAAA;YACV,CAAC;SACD;KACD;IACD,IAAI;QACH,OAAO,EAAE,CAAC;IACX,CAAC;IACD,QAAQ,EAAE;QACT,KAAK;YACJ,OAAO,MAAM,CAAC,IAAI,CAAC,IAAI,CAAC,CAAC,MAAM,GAAG,CAAC,GAAG,EAAE,CAAA;QACzC,CAAC;QACD,UAAU;YACH,MAAA,KAKF,IAAI,EAJP,QAAQ,cAAA,EACR,IAAI,UAAA,EACJ,IAAI,UAAA,EACJ,QAAQ,cACD,CAAA;YACR,OAAO;gBACN,QAAQ,CAAC,CAAC,CAAC,aAAa,GAAG,IAAI,GAAG,WAAW,CAAC,CAAC,CAAC,EAAE;gBAClD,aAAa,GAAG,IAAI;gBACpB,aAAa,GAAG,IAAI;gBACpB,QAAQ,CAAC,CAAC,CAAC,qBAAqB,CAAC,CAAC,CAAC,EAAE;aACrC,CAAC,IAAI,CAAC,GAAG,CAAC,CAAA;QACZ,CAAC;QACD,aAAa;Y