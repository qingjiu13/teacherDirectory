{"code":"import { defineComponent } from \"vue\";\nexport default defineComponent({\n    name: \"group-member-selector\",\n    props: {\n        group: {\n            type: Object\n        },\n        members: {\n            type: Array\n        },\n        maxSize: {\n            type: Number,\n            default: 50\n        }\n    },\n    data() {\n        return {\n            searchText: \"\",\n        };\n    },\n    methods: {\n        init(checkedIds = null, lockedIds = null, hideIds = null) {\n            this.members.forEach((m = null) => {\n                m.checked = checkedIds.indexOf(m.userId) >= 0;\n                m.locked = lockedIds.indexOf(m.userId) >= 0;\n                m.hide = hideIds.indexOf(m.userId) >= 0;\n            });\n        },\n        open() {\n            this.$refs.popup.open();\n        },\n        onSwitchChecked(m = null) {\n            if (!m.locked) {\n                m.checked = !m.checked;\n            }\n            // 达到选择上限\n            if (this.maxSize > 0 && this.checkedIds.length > this.maxSize) {\n                m.checked = false;\n                uni.showToast({\n                    title: `最多选择${this.maxSize}位用户`,\n                    icon: \"none\"\n                });\n            }\n        },\n        onClean() {\n            this.members.forEach((m = null) => {\n                if (!m.locked && m.checked) {\n                    m.checked = false;\n                }\n            });\n        },\n        onOk() {\n            this.$refs.popup.close();\n            this.$emit(\"complete\", this.checkedIds);\n        }\n    },\n    computed: {\n        checkedIds() {\n            return this.checkedMembers.map((m = null) => { return m.userId; });\n        },\n        checkedMembers() {\n            return this.members.filter((m = null) => { return !m.quit && !m.hide && m.checked; });\n        },\n        showMembers() {\n            return this.members.filter((m = null) => { return !m.quit && !m.hide && m.showNickName.includes(this.searchText); });\n        }\n    }\n});\n//# sourceMappingURL=E:/%E7%A0%94%E5%B8%88%E5%BD%95%E4%B8%8E%E5%85%B6%E4%BB%96%E4%BA%BA%E5%85%B1%E4%BA%AB%E6%96%87%E4%BB%B6/teacherDirectory/vue%E5%AE%9E%E7%8E%B0Lottie%E5%8A%A8%E7%94%BB%E4%BB%A3%E7%A0%81/pagesChat/components/group-member-selector/group-member-selector.vue?vue&type=script&lang.uts.js.map","references":[],"uniExtApis":["uni.showToast"],"map":"{\"version\":3,\"file\":\"group-member-selector.vue?vue&type=script&lang.uts.js\",\"sourceRoot\":\"\",\"sources\":[\"group-member-selector.vue?vue&type=script&lang.uts\"],\"names\":[],\"mappings\":\";AACA,+BAAe;IACd,IAAI,EAAE,uBAAuB;IAC7B,KAAK,EAAE;QACN,KAAK,EAAE;YACN,IAAI,EAAE,MAAM;SACZ;QACD,OAAO,EAAE;YACR,IAAI,EAAE,KAAK;SACX;QACD,OAAO,EAAE;YACR,IAAI,EAAE,MAAM;YACZ,OAAO,EAAE,EAAE;SACX;KACD;IACD,IAAI;QACH,OAAO;YACN,UAAU,EAAE,EAAE;SACd,CAAC;IACH,CAAC;IACD,OAAO,EAAE;QACR,IAAI,CAAC,UAAU,OAAA,EAAE,SAAS,OAAA,EAAE,OAAO,OAAA;YAClC,IAAI,CAAC,OAAO,CAAC,OAAO,CAAC,CAAC,CAAC,OAAA;gBACtB,CAAC,CAAC,OAAO,GAAG,UAAU,CAAC,OAAO,CAAC,CAAC,CAAC,MAAM,CAAC,IAAI,CAAC,CAAC;gBAC9C,CAAC,CAAC,MAAM,GAAG,SAAS,CAAC,OAAO,CAAC,CAAC,CAAC,MAAM,CAAC,IAAI,CAAC,CAAC;gBAC5C,CAAC,CAAC,IAAI,GAAG,OAAO,CAAC,OAAO,CAAC,CAAC,CAAC,MAAM,CAAC,IAAI,CAAC,CAAC;YACzC,CAAC,CAAC,CAAC;QACJ,CAAC;QACD,IAAI;YACH,IAAI,CAAC,KAAK,CAAC,KAAK,CAAC,IAAI,EAAE,CAAC;QACzB,CAAC;QACD,eAAe,CAAC,CAAC,OAAA;YAChB,IAAI,CAAC,CAAC,CAAC,MAAM,EAAE;gBACd,CAAC,CAAC,OAAO,GAAG,CAAC,CAAC,CAAC,OAAO,CAAC;aACvB;YACD,SAAS;YACT,IAAI,IAAI,CAAC,OAAO,GAAG,CAAC,IAAI,IAAI,CAAC,UAAU,CAAC,MAAM,GAAG,IAAI,CAAC,OAAO,EAAE;gBAC9D,CAAC,CAAC,OAAO,GAAG,KAAK,CAAC;gBAClB,GAAG,CAAC,SAAS,CAAC;oBACb,KAAK,EAAE,OAAO,IAAI,CAAC,OAAO,KAAK;oBAC/B,IAAI,EAAE,MAAM;iBACZ,CAAC,CAAA;aACF;QACF,CAAC;QACD,OAAO;YACN,IAAI,CAAC,OAAO,CAAC,OAAO,CAAC,CAAC,CAAC,OAAA;gBACtB,IAAI,CAAC,CAAC,CAAC,MAAM,IAAI,CAAC,CAAC,OAAO,EAAE;oBAC3B,CAAC,CAAC,OAAO,GAAG,KAAK,CAAC;iBAClB;YACF,CAAC,CAAC,CAAA;QACH,CAAC;QACD,IAAI;YACH,IAAI,CAAC,KAAK,CAAC,KAAK,CAAC,KAAK,EAAE,CAAC;YACzB,IAAI,CAAC,KAAK,CAAC,U