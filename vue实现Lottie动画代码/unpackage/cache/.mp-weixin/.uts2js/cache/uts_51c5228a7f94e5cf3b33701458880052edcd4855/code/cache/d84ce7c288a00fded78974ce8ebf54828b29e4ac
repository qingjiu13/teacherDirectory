{"code":"import { defineComponent } from \"vue\";\nimport popup from '../uni-popup/popup.js';\nimport { initVueI18n } from '@dcloudio/uni-i18n';\nimport messages from '../uni-popup/i18n/index.js';\nconst t = initVueI18n(messages).t;\n/**\n * PopUp 弹出层-对话框样式\n * @description 弹出层-对话框样式\n * @tutorial https://ext.dcloud.net.cn/plugin?id=329\n * @property {String} value input 模式下的默认值\n * @property {String} placeholder input 模式下输入提示\n * @property {String} type = [success|warning|info|error] 主题样式\n *  @value success 成功\n * \t@value warning 提示\n * \t@value info 消息\n * \t@value error 错误\n * @property {String} mode = [base|input] 模式、\n * \t@value base 基础对话框\n * \t@value input 可输入对话框\n * @property {String} content 对话框内容\n * @property {Boolean} beforeClose 是否拦截取消事件\n * @event {Function} confirm 点击确认按钮触发\n * @event {Function} close 点击取消按钮触发\n */\nexport default defineComponent({\n    name: \"uniPopupDialog\",\n    mixins: [popup],\n    emits: ['confirm', 'close'],\n    props: {\n        inputType: {\n            type: String,\n            default: 'text'\n        },\n        value: {\n            type: [String, Number],\n            default: ''\n        },\n        placeholder: {\n            type: [String, Number],\n            default: ''\n        },\n        type: {\n            type: String,\n            default: 'error'\n        },\n        mode: {\n            type: String,\n            default: 'base'\n        },\n        title: {\n            type: String,\n            default: ''\n        },\n        content: {\n            type: String,\n            default: ''\n        },\n        beforeClose: {\n            type: Boolean,\n            default: false\n        },\n        cancelText: {\n            type: String,\n            default: ''\n        },\n        confirmText: {\n            type: String,\n            default: ''\n        }\n    },\n    data() {\n        return {\n            dialogType: 'error',\n            focus: false,\n            val: \"\"\n        };\n    },\n    computed: {\n        okText() {\n            return this.confirmText || t(\"uni-popup.ok\");\n        },\n        closeText() {\n            return this.cancelText || t(\"uni-popup.cancel\");\n        },\n        placeholderText() {\n            return this.placeholder || t(\"uni-popup.placeholder\");\n        },\n        titleText() {\n            return this.title || t(\"uni-popup.title\");\n        }\n    },\n    watch: {\n        type(val = null) {\n            this.dialogType = val;\n        },\n        mode(val = null) {\n            if (val === 'input') {\n                this.dialogType = 'info';\n            }\n        },\n        value(val = null) {\n            this.val = val;\n        }\n    },\n    created() {\n        // 对话框遮罩不可点击\n        this.popup.disableMask();\n        // this.popup.closeMask()\n        if (this.mode === 'input') {\n            this.dialogType = 'info';\n            this.val = this.value;\n        }\n        else {\n            this.dialogType = this.type;\n        }\n    },\n    mounted() {\n        this.focus = true;\n    },\n    methods: {\n        /**\n         * 点击确认按钮\n         */\n        onOk() {\n            if (this.mode === 'input') {\n                this.$emit('confirm', this.val);\n            }\n            else {\n                this.$emit('confirm');\n            }\n            if (this.beforeClose)\n                return null;\n            this.popup.close();\n        },\n        /**\n         * 点击取消按钮\n         */\n        closeDialog() {\n            this.$emit('close');\n            if (this.beforeClose)\n                return null;\n            this.popup.close();\n        },\n        close() {\n            this.popup.close();\n        }\n    }\n});\n//# sourceMappingURL=E:/%E7%A0%94%E5%B8%88%E5%BD%95%E4%B8%8E%E5%85%B6%E4%BB%96%E4%BA%BA%E5%85%B1%E4%BA%AB%E6%96%87%E4%BB%B6/teacherDirectory/vue%E5%AE%9E%E7%8E%B0Lottie%E5%8A%A8%E7%94%BB%E4%BB%A3%E7%A0%81/uni_modules/uni-popup/components/uni-popup-dialog/uni-popup-dialog.vue?vue&type=script&lang.uts.js.map","references":["E:/研师录与其他人共享文件/teacherDirectory/vue实现Lottie动画代码/uni_modules/uni-popup/components/uni-popup/popup.js","E:/研师录与其他人共享文件/teacherDirectory/vue实现Lottie动画代码/uni_modules/uni-popup/components/uni-popup/i18n/index.js"],"uniExtApis":[],"map":"{\"version\":3,\"file\":\"uni-popup-dialog.vue?vue&type=script&lang.uts.js\",\"sourceRoot\":\"\",\"sources\":[\"uni-popup-dialog.vue?vue&type=script&lang.uts\"],\"names\":[],\"mappings\":\";AACC,OAAO,KAAK,MAAM,uBAAuB,CAAA;AACzC,OAAO,EACP,WAAW,EACV,MAAM,oBAAoB,CAAA;AAC3B,OAAO,QAAQ,MAAM,4BAA4B,CAAA;AACzC,MAAA,CAAC,GAAK,WAAW,CAAC,QAAQ,CAAC,EAA1B,CAA0B;AACnC;;;;;;;;;;;;;;;;;;GAkBG;AAEH,+BAAe;IACd,IAAI,EAAE,gBAAgB;IACtB,MAAM,EAAE,CAAC,KAAK,CAAC;IACf,KAAK,EAAC,CAAC,SAAS,EAAC,OAAO,CAAC;IACzB,KAAK,EAAE;QACN,SAAS,EAAC;YACT,IAAI,EAAE,MAAM;YACZ,OAAO,EAAE,MAAM;SACf;QACD,KAAK,EAAE;YACN,IAAI,EAAE,CAAC,MAAM,EAAE,MAAM,CAAC;YACtB,OAAO,EAAE,EAAE;SACX;QACD,WAAW,EAAE;YACZ,IAAI,EAAE,CAAC,MAAM,EAAE,MAAM,CAAC;YACtB,OAAO,EAAE,EAAE;SACX;QACD,IAAI,EAAE;YACL,IAAI,EAAE,MAAM;YACZ,OAAO,EAAE,OAAO;SAChB;QACD,IAAI,EAAE;YACL,IAAI,EAAE,MAAM;YACZ,OAAO,EAAE,MAAM;SACf;QACD,KAAK,EAAE;YACN,IAAI,EAAE,MAAM;YACZ,OAAO,EAAE,EAAE;SACX;QACD,OAAO,EAAE;YACR,IAAI,EAAE,MAAM;YACZ,OAAO,EAAE,EAAE;SACX;QACD,WAAW,EAAE;YACZ,IAAI,EAAE,OAAO;YACb,OAAO,EAAE,KAAK;SACd;QACD,UAAU,EAAC;YACV,IAAI,EAAE,MAAM;YACZ,OAAO,EAAE,EAAE;SACX;QACD,WAAW,EAAC;YACX,IAAI,EAAE,MAAM;YACZ,OAAO,EAAE,EAAE;SACX;KACD;IACD,IAAI;QACH,OAAO;YACN,UAAU,EAAE,OAAO;YACnB,KAAK,EAAE,KAAK;YACZ,GAAG,EAAE,EAAE;SACP,CAAA;IACF,CAAC;IACD,QAAQ,EAAE;QACT,MAAM;YACL,OAAO,IAAI,CAAC,WAAW,IAAI,CAAC,CAAC,cAAc,CAAC,CAAA;QAC7C,CAAC;QACD,SAAS;YACR,OAAO,IAAI,CAAC,UAAU,IAAI,CAAC,CAAC,kBAAkB,CAAC,CAAA;QAChD,CAAC;QACD,eAAe;YACd,OAAO,IAAI,CAAC,WAAW,IAAI,CAAC,CAAC,uBAAuB,CAAC,CAAA;QACtD,CAAC;QACD,SAAS;YACR,OAAO,IAAI,CAAC,KAAK,IAAI,CAAC,CAAC,iBAAiB,CAAC,CAAA;QAC1C,CAAC;KACD;IACD,KAAK,EAAE;QACN,IAAI,CAAC,GAAG,OAAA;YACP,IAAI,CAAC,UAAU,GAAG,GAAG,CAAA;QACtB,CAAC;QACD,IAAI,CAAC,GAAG,OAAA;YA