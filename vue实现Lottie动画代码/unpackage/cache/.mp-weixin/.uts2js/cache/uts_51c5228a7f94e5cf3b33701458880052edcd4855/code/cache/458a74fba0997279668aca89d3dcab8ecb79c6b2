{"code":"import { defineComponent } from \"vue\";\nimport { MESSAGE_TYPE, MESSAGE_STATUS } from '/pagesChat/common/enums.js';\nimport HeadImage from '/pagesChat/components/head-image/head-image';\nimport LongPressMenu from '/pagesChat/components/long-press-menu/long-press-menu';\nimport Loading from '/pagesChat/components/loading/loading';\nimport ChatGroupReaded from '/pagesChat/components/chat-group-readed/chat-group-readed';\nimport emoUtil from '/pagesChat/common/emotion.js';\nimport urlUtil from '/pagesChat/common/url.js';\nimport * as msgType from '/pagesChat/common/messageType.js';\nimport * as dateUtil from '/pagesChat/common/date.js';\nexport default defineComponent({\n    name: \"chat-message-item\",\n    components: {\n        HeadImage,\n        LongPressMenu,\n        Loading,\n        ChatGroupReaded\n    },\n    props: {\n        headImage: {\n            type: String,\n            required: true\n        },\n        showName: {\n            type: String,\n            required: true\n        },\n        msgInfo: {\n            type: Object,\n            required: true\n        },\n        groupMembers: {\n            type: Array\n        }\n    },\n    data() {\n        return {\n            audioPlayState: 'STOP',\n            innerAudioContext: null,\n            menu: new UTSJSONObject({\n                show: false,\n                style: \"\"\n            }),\n            // 导入的常量，确保即使全局属性不可用也能正常工作\n            MESSAGE_TYPE,\n            MESSAGE_STATUS\n        };\n    },\n    // 确保在模板中可以使用$enums替代方案\n    beforeCreate() {\n        // 先检查全局属性是否可用\n        if (!this.$enums) {\n            // 如果不可用，创建一个替代对象\n            this.$enums = new UTSJSONObject({\n                MESSAGE_TYPE,\n                MESSAGE_STATUS\n            });\n        }\n        // 确保$emo可用\n        if (!this.$emo) {\n            this.$emo = emoUtil;\n        }\n        // 确保$date可用\n        if (!this.$date) {\n            this.$date = dateUtil;\n        }\n        // 确保$url可用\n        if (!this.$url) {\n            this.$url = urlUtil;\n        }\n    },\n    methods: {\n        getSafeImageUrl(content = null) {\n            try {\n                if (!content) {\n                    return '';\n                }\n                // 检查内容是否是JSON格式（以{开头）\n                const trimmed = typeof content === 'string' ? content.trim() : '';\n                if (!trimmed.startsWith('{')) {\n                    uni.__f__('warn', 'at pagesChat/components/chat-message-item/chat-message-item.vue:173', '内容不是JSON格式:', trimmed.substring(0, 10) + '...');\n                    return '';\n                }\n                const parsed = UTS.JSON.parse(content);\n                return (parsed && parsed.thumbUrl) ? parsed.thumbUrl : '';\n            }\n            catch (error) {\n                uni.__f__('error', 'at pagesChat/components/chat-message-item/chat-message-item.vue:180', '解析图片URL出错:', error);\n                return '';\n            }\n        },\n        getAudioDuration(content = null) {\n            try {\n                if (!content) {\n                    return '0\"';\n                }\n                // 检查内容是否是JSON格式（以{开头）\n                const trimmed = content.trim();\n                if (!trimmed.startsWith('{')) {\n                    uni.__f__('warn', 'at pagesChat/components/chat-message-item/chat-message-item.vue:192', '音频内容不是JSON格式:', trimmed.substring(0, 10) + '...');\n                    return '0\"';\n                }\n                const parsed = UTS.JSON.parse(content);\n                return (parsed && parsed.duration) ? parsed.duration + '\"' : '0\"';\n            }\n            catch (error) {\n                uni.__f__('error', 'at pagesChat/components/chat-message-item/chat-message-item.vue:199', '解析音频时长出错:', error);\n                return '0\"';\n            }\n        },\n        onSendFail() {\n            uni.showToast({\n                title: \"该文件已发送失败，目前不支持自动重新发送，建议手动重新发送\",\n                icon: \"none\"\n            });\n        },\n        onPlayAudio() {\n            if (this.audioPlayState == 'PLAYING') {\n                this.stopPlayAudio();\n                return null;\n            }\n            try {\n                if (!this.msgInfo.content) {\n                    uni.showToast({\n                        title: '音频格式错误',\n                        icon: 'none'\n                    });\n                    return null;\n                }\n                let data = UTS.JSON.parse(this.msgInfo.content);\n                let audioUrl = data && data.url ? data.url : null;\n                if (!audioUrl) {\n                    uni.showToast({\n                        title: '音频地址错误',\n                        icon: 'none'\n                    });\n                    return null;\n                }\n                this.innerAudioContext = uni.createInnerAudioContext();\n                this.innerAudioContext.src = audioUrl;\n                this.innerAudioContext.autoplay = true;\n                this.innerAudioContext.onPlay(() => {\n                    this.audioPlayState = 'PLAYING';\n                    this.$emit('audioStateChange', 'PLAYING', this.msgInfo);\n                });\n                this.innerAudioContext.onEnded(() => {\n                    this.audioPlayState = 'STOP';\n                    this.$emit('audioStateChange', 'STOP', this.msgInfo);\n                });\n                this.innerAudioContext.onError((res = null) => {\n                    this.audioPlayState = 'STOP';\n                    uni.__f__('log', 'at pagesChat/components/chat-message-item/chat-message-item.vue:248', res.errMsg);\n                    uni.__f__('log', 'at pagesChat/components/chat-message-item/chat-message-item.vue:249', res.errCode);\n                    this.$emit('audioStateChange', 'STOP', this.msgInfo);\n                });\n                this.innerAudioContext.onStop(() => {\n                    this.audioPlayState = 'STOP';\n                    this.$emit('audioStateChange', 'STOP', this.msgInfo);\n                });\n            }\n            catch (error) {\n                uni.__f__('error', 'at pagesChat/components/chat-message-item/chat-message-item.vue:257', '播放音频出错:', error);\n                uni.showToast({\n                    title: '音频格式错误',\n                    icon: 'none'\n                });\n            }\n        },\n        onSelectMenu(item = null) {\n            switch (item.key) {\n                case 'COPY':\n                    this.$emit('copy', this.msgInfo);\n                    break;\n                case 'DELETE':\n                    this.$emit('delete', this.msgInfo);\n                    break;\n                case 'RECALL':\n                    this.$emit('recall', this.msgInfo);\n                    break;\n                case 'DOWNLOAD':\n                    this.$emit('download', this.msgInfo);\n                    break;\n                default:\n                    break;\n            }\n        },\n        onShowFullImage() {\n            try {\n                if (!this.msgInfo.content) {\n                    uni.showToast({\n                        title: '图片格式错误',\n                        icon: 'none'\n                    });\n                    return null;\n                }\n                let data = UTS.JSON.parse(this.msgInfo.content);\n                let imageUrl = data && data.originUrl ? data.originUrl : null;\n                if (!imageUrl) {\n                    uni.showToast({\n                        title: '图片地址错误',\n                        icon: 'none'\n                    });\n                    return null;\n                }\n                uni.previewImage({\n                    urls: [imageUrl],\n                    current: imageUrl\n                });\n            }\n            catch (error) {\n                uni.__f__('error', 'at pagesChat/components/chat-message-item/chat-message-item.vue:308', '预览图片出错:', error);\n                uni.showToast({\n                    title: '图片格式错误',\n                    icon: 'none'\n                });\n            }\n        },\n        onShowReadedBox() {\n            if (this.msgInfo.groupId) {\n                this.$refs.chatGroupReaded.open();\n            }\n        },\n        stopPlayAudio() {\n            if (this.innerAudioContext) {\n                this.innerAudioContext.stop();\n            }\n            this.audioPlayState = 'STOP';\n        },\n        nodesText() {\n            let color = this.msgInfo.selfSend ? 'white' : '';\n            try {\n                if (!this.msgInfo || !this.msgInfo.content) {\n                    return '';\n                }\n                let text = this.$url ? this.$url.replaceURLWithHTMLLinks(this.msgInfo.content, color) : this.msgInfo.content;\n                return this.$emo ? this.$emo.transform(text, 'emoji-normal') : text;\n            }\n            catch (error) {\n                uni.__f__('error', 'at pagesChat/components/chat-message-item/chat-message-item.vue:335', '处理消息内容出错:', error);\n                return this.msgInfo && this.msgInfo.content ? this.msgInfo.content : '';\n            }\n        }\n    },\n    computed: {\n        loading() {\n            return this.msgInfo.loadStatus && this.msgInfo.loadStatus === \"loading\";\n        },\n        loadFail() {\n            return this.msgInfo.loadStatus && this.msgInfo.loadStatus === \"fail\";\n        },\n        data() {\n            try {\n                if (!this.msgInfo || !this.msgInfo.content) {\n                    return new UTSJSONObject({ name: '无效内容', size: 0 });\n                }\n                const parsed = UTS.JSON.parse(this.msgInfo.content);\n                return parsed || new UTSJSONObject({ name: '无效内容', size: 0 });\n            }\n            catch (error) {\n                uni.__f__('error', 'at pagesChat/components/chat-message-item/chat-message-item.vue:355', '解析消息内容出错:', error);\n                return new UTSJSONObject({ name: '无效内容', size: 0 });\n            }\n        },\n        fileSize() {\n            if (!this.data || typeof this.data.size === 'undefined') {\n                return '0B';\n            }\n            let size = this.data.size;\n            if (size > 1024 * 1024) {\n                return Math.round(size / 1024 / 1024) + \"M\";\n            }\n            if (size > 1024) {\n                return Math.round(size / 1024) + \"KB\";\n            }\n            return size + \"B\";\n        },\n        menuItems() {\n            let items = [];\n            if (this.msgInfo.type == this.MESSAGE_TYPE.TEXT) {\n                items.push({\n                    key: 'COPY',\n                    name: '复制',\n                    icon: 'bars'\n                });\n            }\n            if (this.msgInfo.selfSend && this.msgInfo.id > 0) {\n                items.push({\n                    key: 'RECALL',\n                    name: '撤回',\n                    icon: 'refreshempty'\n                });\n            }\n            items.push({\n                key: 'DELETE',\n                name: '删除',\n                icon: 'trash',\n                color: '#e64e4e'\n            });\n            if (this.msgInfo.type == this.MESSAGE_TYPE.FILE) {\n                items.push({\n                    key: 'DOWNLOAD',\n                    name: '下载并打开',\n                    icon: 'download'\n                });\n            }\n            return items;\n        },\n        isAction() {\n            try {\n                if (!this.msgInfo || typeof this.msgInfo.type === 'undefined') {\n                    return false;\n                }\n                return msgType.isAction(this.msgInfo.type);\n            }\n            catch (error) {\n                uni.__f__('error', 'at pagesChat/components/chat-message-item/chat-message-item.vue:412', '判断消息类型出错:', error);\n                return false;\n            }\n        },\n        isNormal() {\n      