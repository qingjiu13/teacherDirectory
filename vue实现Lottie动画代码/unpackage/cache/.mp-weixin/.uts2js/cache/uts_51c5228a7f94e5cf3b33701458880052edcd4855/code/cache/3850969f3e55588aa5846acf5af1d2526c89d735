data.chatMode) {\n                        this.currentMode = data.data.chatMode;\n                        this.updateContextInfo();\n                    }\n                    // 构建消息列表\n                    const messages = data.data && data.data.messages ? data.data.messages.map((msg = null) => { return (new UTSJSONObject({\n                        role: msg.type === 'user' ? MESSAGE_TYPE.USER : MESSAGE_TYPE.AI,\n                        content: msg.content,\n                        status: MESSAGE_STATUS.SENT,\n                        streaming: false\n                    })); }) : [];\n                    this.messages = messages;\n                    this.closeSidebar();\n                    this.$nextTick(() => {\n                        this.scrollToBottom();\n                    });\n                    uni.__f__('log', 'at pages_AI_Login_Match/AI/AI.vue:860', '旧对话加载完成');\n                    uni.__f__('log', 'at pages_AI_Login_Match/AI/AI.vue:861', '===============');\n                }\n                catch (error) {\n                    uni.__f__('error', 'at pages_AI_Login_Match/AI/AI.vue:864', '处理加载聊天失败:', error);\n                    this.showToast('加载聊天失败', 'none');\n                }\n                finally {\n                    this.toggleLoading(false);\n                }\n            });\n        },\n        /**\n         * @description 处理侧边栏删除聊天事件\n         * @param {Object} data - 删除结果数据\n         */\n        handleDeleteChat(data = null) {\n            if (data.success) {\n                // 如果删除的是当前聊天，开始新聊天\n                if (this.currentChatId === data.chatId) {\n                    this.startNewChat();\n                }\n            }\n            else {\n                this.showToast(data.message || '删除失败', 'none');\n            }\n        },\n        /**\n         * @description 处理侧边栏加载更多事件\n         * @param {Object} data - 加载更多结果数据\n         */\n        handleLoadMore(data = null) {\n            if (!data.success) {\n              