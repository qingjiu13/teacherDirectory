         * @description 发起咨询\n         */\n        startConsultation() {\n            this.isLoading = true;\n            setTimeout(() => {\n                this.isLoading = false;\n                // 使用router.js中的方法跳转到聊天页面\n                Navigator.toChat(this.teacherId);\n            }, 800);\n        }\n    })\n});\n//# sourceMappingURL=E:/yanshilu%E9%87%8D%E6%96%B0%E6%9D%A5%E8%BF%87/%E9%92%88%E5%AF%B9%E6%97%A0%E6%B3%95%E5%90%AF%E5%8A%A8%E7%9A%84%E4%BF%AE%E6%94%B9/pages/teacher/teacher.vue?vue&type=script&lang.uts.js.map","references":["E:/yanshilu重新来过/针对无法启动的修改/router/Router.js","E:/yanshilu重新来过/针对无法启动的修改/store/index.js","E:/HBuilderX/HBuilderX.4.45.2025010502/HBuilderX/plugins/uniapp-cli-vite/node_modules/vuex/types/index.d.ts"],"uniExtApis":[],"map":"{\"version\":3,\"file\":\"teacher.vue?vue&type=script&lang.uts.js\",\"sourceRoot\":\"\",\"sources\":[\"teacher.vue?vue&type=script&lang.uts\"],\"names\":[],\"mappings\":\";;AACI,OAAO,EAAE,SAAS,EAAE,aAAa,EAAE,MAAM,oBAAoB,CAAA;AAC7D,OAAO,KAAK,MAAM,kBAAkB,CAAA;AACpC,OAAO,EAAE,UAAU,EAAE,MAAM,MAAM,CAAA;AACjC;;;EAGE;AACF,+BAAe;IACX,IAAI;QACJ,OAAO;YACH,SAAS,EAAE,IAAI;YACf,SAAS,EAAE,UAAU;YACrB,SAAS,EAAE,KAAK,CAAC,SAAS;SAC7B,CAAA;IACD,CAAC;IAEL,QAAQ,oDAKD,UAAU,CAAC,OAAO,EAAE,CAAC,aAAa,CAAC,CAAC;QAEvC;;;UAGE;QACF,OAAO;YACH,MAAM,WAAW,GAAG,IAAI,CAAC,WAAW,CAAC,IAAI,CAAC,SAAS,CAAC,CAAC;YACrD,OAAO,WAAW,sBAAI;gBAClB,EAAE,EAAE,IAAI;gBACR,IAAI,EAAE,EAAE;gBACR,MAAM,EAAE,0CAA0C;gBAClD,MAAM,EAAE,EAAE;gBACV,KAAK,EAAE,EAAE;gBACT,YAAY,EAAE,CAAC;gBACf,WAAW,EAAE,CAAC;gBACd,gBAAgB,EAAE,QAAQ;aAC7B,CAAA,CAAC;QACN,CAAC;QAED;;;UAGE;QACF,QAAQ;YACJ,IAAI,CAAC,IAAI,CAAC,OAAO,IAAI,CAAC,IAAI,CAAC,OAAO,CAAC,OAAO,EAAE;gBACxC,OAAO,EAAE,CAAC;aACb;YACD,OAAO,IAAI,CAAC,OAAO,CAAC,OAAO,IAAI,EAAE,CAAC;QACtC,CAAC,IACJ;IAEK,MAAM,CAAC,OAAO;;YAClB,SAAS;YACT,IAAI,CAAC,SAAS,GAAG,OAAO,CAAC,EAAE,IAAI,EAAE,CAAC;YAElC,SAAS;YACT,IAAI,CAAC,SAAS,GAAG,IAAI,CAAC;YAEtB,aAAa;YACb,IAAI,CAAC,SAAS,GAAG,KAAK,CAAC;QACzB,CAAC;KAAA;IAED,OAAO,oBAAE;QACP;;;WAGG;QACH,SAAS,CAAC,GAAG,OA