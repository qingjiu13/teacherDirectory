:host{display:flex;flex-direction:column}
.tab-bar-wrapper{width:100%;height:160rpx;position:fixed;bottom:0;left:0;right:0;z-index:999;overflow:visible}.tab-bar{display:flex;flex-direction:row;width:100%;height:160rpx;border-top:1px solid #eeeeee;background:linear-gradient(180deg,#e9eaff 11.54%,#747aff 111.54%);overflow:visible}.index-tab{width:40%;position:relative;align-items:center;justify-content:flex-end;overflow:visible}.tab-item{width:auto;padding:0 20rpx;height:110rpx;display:flex;flex-direction:column;justify-content:center;align-items:center;overflow:visible}.nav-icon{width:48rpx;height:48rpx;margin-bottom:6rpx}.index-tab .nav-icon{width:232rpx;height:140rpx;position:absolute;top:-50rpx;left:50%;transform:translate(-50%);z-index:1000}.tab-text{font-family:PingFang SC,sans-serif;font-weight:400;font-size:20rpx;line-height:100%;letter-spacing:-1.26rpx;text-align:center;color:#000}.index-tab .tab-text{top:15rpx;font-weight:700}.active .tab-text{color:#5f26f7}.tab-item.ai-tab{width:20%}.tab-item.index-tab{width:40%}.tab-item.message-tab,.tab-item.mine-tab{width:20%}
