:host{display:flex;flex-direction:column}
.tab-bar-wrapper{width:100%;height:120rpx;position:fixed;bottom:0;left:0;right:0;z-index:999;overflow:visible}.tab-bar{display:flex;flex-direction:row;width:100%;height:120rpx;border-top:1px solid #eeeeee;background:linear-gradient(180deg,#e9eaff 11.54%,#747aff 111.54%);overflow:visible}.tab-item{width:auto;padding:0 20rpx;height:110rpx;display:flex;flex-direction:column;justify-content:center;align-items:center;overflow:visible}.nav-icon{width:48rpx;height:48rpx;margin-bottom:6rpx}.tab-text{font-family:PingFang SC;font-weight:400;font-size:20rpx;line-height:100%;letter-spacing:-1.26rpx;text-align:center;color:#979797}.active .tab-text{color:#5f26f7}.tab-item.ai-tab,.tab-item.index-tab,.tab-item.message-tab,.tab-item.mine-tab{width:25%}
