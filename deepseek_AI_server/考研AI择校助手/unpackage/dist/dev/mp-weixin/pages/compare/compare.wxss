
.container {
	padding: 20rpx;
}
.header {
	margin-bottom: 20rpx;
}
.title {
	font-size: 40rpx;
	font-weight: bold;
}
.compare-list {
	margin-top: 20rpx;
}
.compare-item {
	padding: 20rpx;
	border-bottom: 1px solid #eee;
}
.compare-name {
	font-size: 32rpx;
	font-weight: bold;
}
.compare-desc {
	font-size: 28rpx;
	color: #666;
}
.compare-btn {
	margin-top: 20rpx;
	background-color: #007aff;
	color: #fff;
	border-radius: 30rpx;
}

