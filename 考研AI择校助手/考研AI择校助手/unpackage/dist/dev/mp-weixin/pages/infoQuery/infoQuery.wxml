<view class="container"><view class="header"><text class="title">信息查询</text></view><view class="search-box"><input class="search-input" placeholder="请输入查询内容" data-event-opts="{{[['input',[['__set_model',['','searchText','$event',[]]]]]]}}" value="{{searchText}}" bindinput="__e"/><button data-event-opts="{{[['tap',[['handleSearch',['$event']]]]]}}" class="search-btn" bindtap="__e">搜索</button></view><view class="result-list"><block wx:for="{{resultList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="result-item"><text class="result-title">{{item.title}}</text><text class="result-content">{{item.content}}</text></view></block></view></view>