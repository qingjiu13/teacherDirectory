<view class="container"><view class="header"><text class="title">同考试科目查询</text></view><view class="search-box"><input class="search-input" placeholder="请输入专业或科目关键词" data-event-opts="{{[['confirm',[['handleSearch',['$event']]]],['input',[['__set_model',['','searchText','$event',[]]]]]]}}" value="{{searchText}}" bindconfirm="__e" bindinput="__e"/><button data-event-opts="{{[['tap',[['handleSearch',['$event']]]]]}}" class="search-btn" bindtap="__e">搜索</button></view><block wx:if="{{$root.g0>0}}"><view class="result-list"><block wx:for="{{examList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="exam-item"><text class="major-name">{{item.major}}</text><view class="subject-list"><block wx:for="{{item.subjects}}" wx:for-item="subject" wx:for-index="sIndex" wx:key="sIndex"><text class="subject-item">{{''+subject+''}}</text></block></view></view></block></view></block><block wx:else><view class="empty-tip"><text>暂无相关数据</text></view></block></view>